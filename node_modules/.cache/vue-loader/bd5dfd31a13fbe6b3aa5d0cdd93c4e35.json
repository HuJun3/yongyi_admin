g\">-->\n<!--                        <img v-lazy=\"row.avatar\">-->\n<!--                    </div>-->\n<!--                </viewer>-->\n<!--            </template>-->\n<!--        </Table>-->\n        <div class=\"list-wrapper\">\n            <div class=\"user-item\" v-for=\"(item,index) in labelLists\" :key=\"index\" @click=\"bindActive(item)\">\n                <img v-lazy=\"item.avatar\" alt=\"\">\n                <p class=\"line1\">{{item.wx_name}}</p>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\n    import { transferList,serviceTransfer } from '@/api/kefu'\n    export default {\n        name: \"transfer\",\n        props:{\n            userUid:{\n                type:String | Number,\n                default:''\n            }\n        },\n        data(){\n            return{\n                loading: false,\n                currentChoose:'',\n                labelLists: [],\n                name:'',\n            }\n        },\n        mounted() {\n            this.getList()\n        },\n        methods:{\n            getList(){\n                transferList({\n                    nickname:this.name,\n                    uid:this.userUid\n                }).then(res=>{\n                    this.labelLists = res.data.list\n                })\n            },\n            bindActive(item){\n                // this.$emit('transferPeople',item)\n                serviceTransfer({\n                    uid:this.userUid,\n                    kefuToUid:item.uid\n                }).then(res=>{\n                    this.$Message.success(res.msg)\n                    this.$emit('close')\n                }).catch(error=>{\n                    this.$Message.error(error.msg)\n                })\n            }\n        }\n    }\n</script>\n\n<style lang=\"stylus\" scoped>\n.list-wrapper\n    .user-item\n        display flex\n        align-items center\n        margin-bottom 12px\n        cursor pointer\n        &:last-child\n            margin-bottom 0\n        img\n            width 32px\n            height 32px\n            border-radius 50%\n            margin-right 8px\n        p\n            width 80%\n            color #333\n            font-size 13px\n</style>\n"]}]}