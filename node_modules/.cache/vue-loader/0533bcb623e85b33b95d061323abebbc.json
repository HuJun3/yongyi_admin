   }\n        },\n        data () {\n            return {\n                defaults: {},\n                sliderWidth: 0,\n                datas: this.configData\n            }\n        },\n        mounted () {},\n        watch: {\n             configData: {\n                handler (nVal, oVal) {\n                    this.datas = nVal\n                },\n                immediate: true,\n                deep: true\n            }\n        },\n        methods: {\n            numberVal(val){\n                let r = /^\\+?[1-9][0-9]*$/;\n                if(!r.test(val)){\n                    this.datas[this.name].val = 3;\n                }\n            },\n            maxNum(val){\n                if(val>=50){\n                    this.datas[this.name].val = 50;\n                }\n            }\n        }\n    }\n</script>\n\n<style scoped lang=\"stylus\">\n    .numbox\n        display flex\n        align-items center\n        margin 20px 0 10px 0\n        span\n            width 80px\n            color #999\n    /*font-size 12px*/\n    .c_row-item\n        width 100%\n</style>\n"]}]}