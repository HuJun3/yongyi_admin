         <div class=\"header acea-row row-middle\">\n                    <div class=\"pictrue\"><img :src=\"spread.avatar\"></div>\n                    <div class=\"name\">{{spread.nickname}}</div>\n                </div>\n                <div class=\"list\">\n                    <div class=\"item acea-row row-middle\">\n                        <div class=\"name money ivu-form-item-content\">金额：<span>{{spread.now_money}}</span></div>\n                        <div class=\"name ivu-form-item-content\">UID：<span>{{spread.uid}}</span></div>\n                    </div>\n                    <div class=\"item acea-row row-middle\">\n                        <div class=\"name commission ivu-form-item-content\">佣金：<span>{{spread.brokerage_price}}</span></div>\n                        <div class=\"name ivu-form-item-content\">真实姓名：<span>{{spread.real_name}}</span></div>\n                    </div>\n                    <div class=\"item acea-row row-middle\">\n                        <div class=\"name ivu-form-item-content\">身份证：<span>{{spread.card_id}}</span></div>\n                        <div class=\"name ivu-form-item-content\">手机号码：<span>{{spread.phone}}</span></div>\n                    </div>\n                    <div class=\"item acea-row row-middle\">\n                        <div class=\"name ivu-form-item-content\">生日：<span>{{spread.birthday}}</span></div>\n                        <div class=\"name ivu-form-item-content\">积分：<span>{{spread.integral}}</span></div>\n                    </div>\n                    <div class=\"item acea-row row-middle\">\n                        <div class=\"name ivu-form-item-content\">用户备注：<span>{{spread.mark}}</span></div>\n                        <div class=\"name ivu-form-item-content\">最后登录时间：<span>{{spread.last_time}}</span></div>\n                    </div>\n                </div>\n            </div>\n            <div slot=\"footer\"></div>\n            <Spin size=\"large\" fix v-if=\"spinShow\"></Spin>\n        </Modal>\n    </div>\n</template>\n\n<script>\nimport { verifySpreadInfoApi } from '@/api/setting'\nexport default {\n  name: 'referrerInfo',\n  data () {\n    return {\n      isTemplate: false,\n      spread: {},\n      spinShow: false\n    }\n  },\n  mounted () {\n  },\n  methods: {\n    verifySpreadInfo (uid) {\n      let that = this\n      that.spinShow = true\n      verifySpreadInfoApi(uid).then(res => {\n        that.spinShow = false\n        that.spread = res.data.spread\n      }).catch(res => {\n        that.$Message.error(res.msg)\n      })\n    },\n    cancel () {\n\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"stylus\">\n    .Modals\n       width 100%\n       border 1px solid #e8eaec\n    .Modals .header\n        background-color #f5f5f5\n        padding 10px 15px\n    .Modals .header .pictrue\n        width 50px\n        height 50px\n        border-radius 50%\n    .Modals .header .pictrue img\n        width 100%\n        height 100%\n        border-radius 50%\n    .Modals .header .name\n        color #333\n        margin-left 15px\n    .Modals .list .item .name.money\n        color #ff0005 !important\n    .Modals .list .item .name.commission\n        color green !important\n    .Modals .list .item\n        border-top 1px solid #e8eaec\n    .Modals .list .item .name\n        padding 10px 15px\n        width 50%\n</style>\n"],"sourceRoot":"src/components/referrerInfo"}]}