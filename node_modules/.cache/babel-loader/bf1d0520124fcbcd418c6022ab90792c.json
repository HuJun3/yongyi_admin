,OArCA;AA0CA,MAAA,UAAA,EAAA,EA1CA;AA2CA,MAAA,KAAA,EAAA;AA3CA,KAAA;AA6CA,GAhDA;AAiDA,EAAA,QAAA,kCACA,QAAA,CAAA,OAAA,EAAA,CACA,UADA,CAAA,CADA;AAIA,IAAA,UAJA,wBAIA;AACA,aAAA,KAAA,QAAA,GAAA,SAAA,GAAA,EAAA;AACA,KANA;AAOA,IAAA,aAPA,2BAOA;AACA,aAAA,KAAA,QAAA,GAAA,KAAA,GAAA,OAAA;AACA;AATA,IAjDA;AA4DA,EAAA,OA5DA,qBA4DA;AACA,SAAA,OAAA;AACA,GA9DA;AA+DA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,GAFA,iBAEA;AAAA;;AACA,WAAA,UAAA,CAAA,aAAA,CAAA,CAAA,CAAA,EAAA,IAAA,CAAA;AAAA,eAAA,KAAA,CAAA,OAAA,EAAA;AAAA,OAAA;AACA,KAJA;AAKA;AACA,IAAA,IANA,gBAMA,EANA,EAMA;AAAA;;AACA,WAAA,UAAA,CAAA,aAAA,CAAA,EAAA,CAAA,EAAA,IAAA,CAAA;AAAA,eAAA,MAAA,CAAA,OAAA,EAAA;AAAA,OAAA;AACA,KARA;AASA;AACA,IAAA,GAVA,eAUA,GAVA,EAUA,GAVA,EAUA,GAVA,EAUA;AAAA;;AACA,UAAA,WAAA,GAAA;AACA,QAAA,KAAA,EAAA,GADA;AAEA,QAAA,GAAA,EAAA,GAFA;AAGA,QAAA,GAAA,4BAAA,GAAA,CAAA,EAAA,CAHA;AAIA,QAAA,MAAA,EAAA,QAJA;AAKA,QAAA,GAAA,EAAA;AALA,OAAA;AAOA,WAAA,UAAA,CAAA,WAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,GAAA,CAAA,GAAA;;AACA,QAAA,MAAA,CAAA,UAAA,CAAA,MAAA,CAAA,GAAA,EAAA,CAAA;;AAEA,QAAA,MAAA,CAAA,OAAA;AACA,OALA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAPA;AAQA,KA1BA;AA2BA;AACA,IAAA,OA5BA,qBA4BA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,MAAA,cAAA,CAAA,KAAA,SAAA,CAAA,CAAA,IAAA;AAAA,2EAAA,iBAAA,GAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,kBAAA,IADA,GACA,GAAA,CAAA,IADA;AAEA,kBAAA,MAAA,CAAA,UAAA,GAAA,IAAA,CAAA,IAAA;AACA,kBAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,KAAA;AACA,kBAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AAJA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA,WAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;;AACA,QAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA,OARA;AASA,KAvCA;AAwCA,IAAA,UAxCA,sBAwCA,KAxCA,EAwCA;AACA,WAAA,SAAA,CAAA,IAAA,GAAA,KAAA;AACA,WAAA,OAAA;AACA;AA3CA;AA/DA,CAAA","sourcesContent":["<template>\n    <div>\n        <div class=\"i-layout-page-header\">\n            <span class=\"ivu-page-header-title\">{{$route.meta.title}}</span>\n        </div>\n\n        <Card :bordered=\"false\" dis-hover class=\"ivu-mt\">\n            <Row type=\"flex\">\n                <Col v-bind=\"grid\">\n                    <Button v-auth=\"['admin-user-label_add']\" type=\"primary\"  icon=\"md-add\" @click=\"add\">添加主播</Button>\n                </Col>\n            </Row>\n            <Table :columns=\"columns1\" :data=\"labelLists\" ref=\"table\" class=\"mt25\"\n                   :loading=\"loading\" highlight-row\n                   no-userFrom-text=\"暂无数据\"\n                   no-filtered-userFrom-text=\"暂无筛选结果\">\n                <template slot-scope=\"{ row, index }\" slot=\"icons\">\n                    <viewer>\n                        <div class=\"tabBox_img\">\n                            <img v-lazy=\"row.icon\">\n                        </div>\n                    </viewer>\n                </template>\n                <template slot-scope=\"{ row, index }\" slot=\"action\">\n                    <a @click=\"edit(row.id)\">修改</a>\n                    <Divider type=\"vertical\" />\n                    <a @click=\"del(row,'删除分组',index)\">删除</a>\n                </template>\n            </Table>\n            <div class=\"acea-row row-right page\">\n                <Page :total=\"total\" show-elevator show-total @on-change=\"pageChange\"\n                      :page-size=\"labelFrom.limit\"/>\n            </div>\n        </Card>\n    </div>\n</template>\n\n<script>\n    import { mapState } from 'vuex';\n    import { liveAuchorList,liveAuchorAdd,liveAuchorDel } from '@/api/live'\n    export default {\n        name: \"anchor\",\n        data () {\n            return {\n                grid: {\n                    xl: 7,\n                    lg: 7,\n                    md: 12,\n                    sm: 24,\n                    xs: 24\n                },\n                loading: false,\n                columns1: [\n                    {\n                        title: 'ID',\n                        key: 'id',\n                        minWidth: 120\n                    },\n                    {\n                        title: '名称',\n                        key: 'name',\n                        minWidth: 300\n                    },\n                    {\n                        title: '电话',\n                        key: 'phone',\n                        minWidth: 300\n                    },\n                    {\n                        title: '微信号',\n                        key: 'wechat',\n                        minWidth: 300\n                    },\n                    {\n                        title: '操作',\n                        slot: 'action',\n                        fixed: 'right',\n                        minWidth: 120\n                    }\n                ],\n                labelFrom: {\n                    kerword:'',\n                    page: 1,\n                    limit: 15\n                },\n                labelLists: [],\n                total: 0\n            }\n        },\n        computed: {\n            ...mapState('media', [\n                'isMobile'\n            ]),\n            labelWidth () {\n                return this.isMobile ? undefined : 75;\n            },\n            labelPosition () {\n                return this.isMobile ? 'top' : 'right';\n            }\n        },\n        created () {\n            this.getList();\n        },\n        methods:{\n            // 添加\n            add () {\n                this.$modalForm(liveAuchorAdd(0)).then(() => this.getList());\n            },\n            // 修改\n            edit(id){\n                this.$modalForm(liveAuchorAdd(id)).then(() => this.getList());\n            },\n            // 删除\n            del(row, tit, num){\n                let delfromData = {\n                    title: tit,\n                    num: num,\n                    url: `live/anchor/del/${row.id}`,\n                    method: 'DELETE',\n                    ids: ''\n                };\n                this.$modalSure(delfromData).then((res) => {\n                    this.$Message.success(res.msg);\n                    this.labelLists.splice(num, 1);\n\n                    this.getList();\n                }).catch(res => {\n                    this.$Message.error(res.msg);\n                });\n            },\n            // 分组列表\n            getList () {\n                this.loading = true;\n                liveAuchorList(this.labelFrom).then(async res => {\n                    let data = res.data;\n                    this.labelLists = data.list;\n                    this.total = data.count;\n                    this.loading = false;\n                }).catch(res => {\n                    this.loading = false;\n                    this.$Message.error(res.msg);\n                })\n            },\n            pageChange (index) {\n                this.labelFrom.page = index;\n                this.getList();\n            },\n        }\n    }\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/pages/marketing/live"}]}